{
    "venvPath": ".",
    "venv": ".venv",
    "pythonVersion": "3.13",
    "pythonPlatform": "Darwin",
    "executionEnvironments": [
        {
            "root": ".",
            "pythonVersion": "3.13",
            "pythonPlatform": "Darwin",
            "extraPaths": [
                ".venv/lib/python3.13/site-packages"
            ]
        }
    ],
    "exclude": [
        "**/__pycache__",
        "**/third_party",
        "**/.venv",
        "**/venv",
        "**/venv3.11",
        "**/node_modules",
        "web/venv/**",
        "ukrainian-tts/venv3.11/**"
    ],
    "reportMissingImports": "warning",
    "reportMissingTypeStubs": false
}